{
    "source": {
        "include": ["./"],
        "includePattern": ".js$",
        "excludePattern": "(node_modules/|docs)"
    },
    "recurseDepth": 10,
    "plugins": [
        "plugins/markdown"
    ],
    "opts": {
        "encoding": "utf8",
        "readme": "./readme.md",
        "destination": "docs/",
        "recurse": true,
        "verbose": true,
        "middlewares": "./middlewares",
        "routes": "./routes"
    }
}